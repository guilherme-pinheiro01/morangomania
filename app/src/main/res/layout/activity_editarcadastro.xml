<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/descricao_imagem_cadastro"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/img_editar_cadastro"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="149dp"
        android:layout_height="20dp"
        android:text="Nome*"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="232dp"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/editTextText1"
        android:layout_width="311dp"
        android:layout_height="50dp"
        android:background="@drawable/caixanome"
        android:ems="10"
        android:hint="Insira seu nome"
        android:inputType="text"
        android:padding="16dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="252dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="83dp"
        android:layout_height="20dp"
        android:text="Telefone*"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="192dp"
        tools:layout_editor_absoluteY="312dp"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/editTextText3"
        android:layout_width="145dp"
        android:layout_height="50dp"
        android:background="@drawable/caixatelefone"
        android:ems="10"
        android:hint="Digite seu telefone"
        android:inputType="text"
        android:padding="15dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="190dp"
        tools:layout_editor_absoluteY="332dp"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/editTextText2"
        android:layout_width="143dp"
        android:layout_height="53dp"
        android:background="@drawable/caixacpf2"
        android:ems="10"
        android:hint="Digite seu CPF"
        android:inputType="text"
        android:padding="15dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="332dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="77dp"
        android:layout_height="20dp"
        android:text="Email*"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="384dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="72dp"
        android:layout_height="28dp"
        android:text="Senha*"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="192dp"
        tools:layout_editor_absoluteY="384dp"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/editTextText6"
        android:layout_width="151dp"
        android:layout_height="50dp"
        android:background="@drawable/caixasenha"
        android:ems="10"
        android:hint="Digite sua senha"
        android:inputType="text"
        android:padding="12dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="189dp"
        tools:layout_editor_absoluteY="404dp"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/editTextText5"
        android:layout_width="144dp"
        android:layout_height="51dp"
        android:background="@drawable/caixaemail"
        android:ems="10"
        android:hint="Digite o email"
        android:inputType="text"
        android:padding="15dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="404dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="79dp"
        android:layout_height="19dp"
        android:text="CPF *"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="312dp"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button1"
        android:layout_width="88dp"
        android:layout_height="49dp"
        android:background="@drawable/btn_excluir"
        android:text="Excluir"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="534dp"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button2"
        android:layout_width="85dp"
        android:layout_height="48dp"
        android:background="@drawable/btn_cancelar"
        android:text="Cancelar"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="535dp"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>